{
  "name": "books-react-redux-app",
  "version": "0.0.1",
  "description": "books react redux app",
  "main": "src/index.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "./node_modules/.bin/nodemon ./bin/www",
    "dev": "concurrently \"npm start\" \"npm run start:dev\"",
    "build": "rimraf dist && webpack --config ./webpack.prod.config.js --progress --profile --colors",
    "static:start": "node_modules/pm2/bin/pm2 start static.server.js --name='static'",
    "static:restart": "node_modules/pm2/bin/pm2 restart static",
    "static:stop": "node_modules/pm2/bin/pm2 stop static",
    "deploy": "npm run build && npm run static:restart",
    "deploy:i": "npm i && npm run build && npm run static:restart",
    "list": "node_modules/pm2/bin/pm2 list",
    "lint": "eslint src"
  },
  "moduleRoots": [
    "src"
  ],
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.16.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "morgan": "^1.7.0",
    "normalizr": "^3.1.0",
    "pm2": "^2.2.3",
    "rc-slider": "^5.4.3",
    "react": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-autosuggest": "^7.0.2",
    "react-bootstrap": "^0.30.7",
    "react-dates": "^4.3.0",
    "react-dom": "^15.4.1",
    "react-native-web": "0.0.68",
    "react-page-click": "^3.0.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-form": "^6.4.3",
    "redux-logger": "^2.7.4",
    "redux-saga": "^0.14.1",
    "reselect": "^2.5.4",
    "superagent": "^3.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.16.1",
    "http-proxy-middleware": "^0.17.3",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "node-sass": "^3.13.1",
    "nodemon": "^1.11.0",
    "postcss-import": "^9.0.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
